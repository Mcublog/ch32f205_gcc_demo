{
    "version": 4,
    "configurations": [
        {
            "name": "Application",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "ARM_MATH_CM3",
                "USE_STDPERIPH_DRIVER"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ]
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "configurationProvider": "go2sh.cmake-integration"
        }
    ]
}